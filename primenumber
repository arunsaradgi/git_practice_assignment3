function primeNumber(num) {

  let count = 0;
  for (let i = 2; i <= num/2; i++) {
    if (num % i == 0) {
      count++;
    }
  }
  if (count == 0) {
    return true;
  } else {
    return false;
  }

}
let num = 16;
if (primeNumber(num) == true) {
  console.log("prime number");
} else {
  console.log("not a prime number");
}